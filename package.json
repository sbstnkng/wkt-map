{
  "name": "wkt-map",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://sbstnkng.github.io/wkt-map/",
  "dependencies": {
    "@terraformer/wkt": "^2.2.1",
    "bootstrap": "^5.3.3",
    "leaflet": "^1.9.4",
    "object-hash": "^3.0.0",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-leaflet": "^3.2.5",
    "react-redux": "^9.2.0",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "typescript": "^5.6.3",
    "uuid": "^13.0.0",
    "web-vitals": "^4.2.4"
  },
  "overrides": {
    "typescript": "^5.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^22.13.10",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.2",
    "@types/react-redux": "^7.1.34",
    "@types/uuid": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3"
  }
}
